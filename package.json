{
  "name": "playwright-typescript-ts-node-jasmine-expect",
  "version": "1.0.0",
  "description": "Playwright end-to-end test automation boilerplate, using ES6 Modules, TypeScript, Jasmine and Expect.",
  "scripts": {
    "pretty": "prettier --no-config './spec/*.ts' --write",
    "lint": "tslint -c tslint.json './spec/*.ts'",
    "test": "npx jasmine"
  },
  "keywords": [
    "playwright",
    "es6",
    "typescript",
    "tsnode",
    "jasmine",
    "boilerplate",
    "example",
    "e2e",
    "e2e test"
  ],
  "author": "Girma Nigusse <xgirma@gmail.com> (https://github.com/xgirma)",
  "license": "MIT",
  "devDependencies": {
    "@types/is-ci": "2.0.0",
    "@types/jasmine": "3.5.5",
    "@types/node": "12.12.27",
    "husky": "3.1.0",
    "jasmine": "3.5.0",
    "lint-staged": "10.0.7",
    "playwright": "0.11.1",
    "prettier": "1.19.1",
    "ts-node": "8.6.2",
    "tslint": "5.20.1",
    "tslint-plugin-prettier": "2.1.0",
    "typescript": "3.7.5"
  },
  "lint-staged": {
    "*.{ts,js,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
